<?xml version="1.0" encoding="UTF-8"?>
<sections xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="urn:magento:mftf:Page/etc/SectionObject.xsd">
    <section name="AdminGroupcatGetAQuoteRequestsGridSection">
        <element name="Email" type="text" selector="//td[@data-column='email'][contains(string(), 'email1@example.com')]"/>
        <element name="Name" type="text" selector="//td[@data-column='name'][contains(string(), 'ww ww')]"/>
        <element name="EmailFilter" type="text" selector="//input[@type='text'][@id='amasty_groupcat_rule_grid_filter_email']"/>
        <element name="Search" type="button" selector="//button[@title='Search'][contains(string(), 'Search')]"/>
        <element name="ResetFilter" type="button" selector="//button[@data-action='grid-filter-reset']"/>
        <element name="RecordsFoundMessage" type="text" selector="//span[@id='amasty_groupcat_rule_grid-total-count'][contains(string(), '1')]"/>
        <element name="FirstRowCheckbox" type="checkbox" selector="//tr//td//label//input[contains(@data-role, 'select-row')]"/>
        <element name="MassAction" type="button" selector="//*[@id='amasty_groupcat_rule_grid_massaction-select']//*[@class='admin__control-select-placeholder']"/>
        <element name="DeleteMassAction" type="button" selector="//*[@class='required-entry local-validation admin__control-select ']//*[@value='change_mode_onthefly'][contains(string(), 'Delete')]"/>
        <element name="SubmitButton" type="button" selector="//button[@title='Submit']"/>
        <element name="AcceptDeleteButton" type="button" selector="//*[@class='action-primary action-accept']"/>
        <element name="RuleSuccessfullyRemovedMessage" type="text" selector="//*[@data-ui-id='messages-message-success'][contains(string(), 'Get a Quote requests are deleted.')]"/>
    </section>
</sections>
