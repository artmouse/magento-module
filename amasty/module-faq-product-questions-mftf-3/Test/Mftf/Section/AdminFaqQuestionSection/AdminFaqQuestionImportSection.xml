<?xml version="1.0" encoding="UTF-8"?>
<sections xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:mftf:Page/etc/SectionObject.xsd">

    <section name="AdminFaqQuestionImportSection">
        <element name="ImportBehavior" type="select" selector="#faqquestionbasic_behavior"/>
        <element name="SelectFileToImport" type="input" selector="#import_file"/>
        <element name="CheckData" type="button" selector="#upload_button"/>
        <element name="CheckedDataMsg" type="text" parameterized="true" selector="//div[@class='message message-notice notice'][contains(string(), '{{message}}')]"/>
        <element name="FileValidMsg" type="text" selector="//div[@class='message message-success success'][contains(string(), 'File is valid! To start import process press &quot;Import&quot; button')]"/>
        <element name="ImportButton" type="button" selector="#import_validation_container button"/>
        <element name="ImportSuccess" type="text" selector="//div[@class='message message-success success'][contains(string(), 'Import successfully done')]"/>
    </section>
</sections>
