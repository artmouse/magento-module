<?xml version="1.0" encoding="UTF-8"?>
<actionGroups xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:mftf:Test/etc/actionGroupSchema.xsd">

    <actionGroup name="StorefrontAssertNoCategoryOnFaqPageActionGroup">
        <arguments>
            <argument name="categoryTitle" defaultValue="defaultCategory.title"/>
        </arguments>
        <dontSeeElement selector="{{StorefrontFaqSection.CategoryTitle(categoryTitle)}}" stepKey="assertCategoryTitle"/>
    </actionGroup>
</actionGroups>
