<?xml version="1.0" encoding="UTF-8"?>
<actionGroups xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:mftf:Test/etc/actionGroupSchema.xsd">

    <actionGroup name="StorefrontAssertCategoryIconOnFaqPageActionGroup">
        <arguments>
            <argument name="category" defaultValue="defaultCategory"/>
        </arguments>
        <seeElement selector="{{StorefrontFaqSection.CategoryIcon(category.title, category.iconFileName)}}" stepKey="assertCategoryIcon"/>
    </actionGroup>
</actionGroups>
