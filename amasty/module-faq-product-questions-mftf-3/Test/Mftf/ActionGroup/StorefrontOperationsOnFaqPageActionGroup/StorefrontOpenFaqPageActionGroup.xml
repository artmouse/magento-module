<?xml version="1.0" encoding="UTF-8"?>
<actionGroups xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:mftf:Test/etc/actionGroupSchema.xsd">

    <actionGroup name="StorefrontOpenFaqPageActionGroup">
        <arguments>
            <argument name="url" defaultValue="defGeneralSettings.urlPrefix"/>
        </arguments>
        <amOnPage url="{{url}}" stepKey="openStorefrontFaqPage"/>
        <waitForPageLoad stepKey="waitForStorefrontFaqPageLoad"/>
    </actionGroup>
</actionGroups>
