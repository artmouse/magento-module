<?xml version="1.0" encoding="UTF-8"?>
<entities xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="urn:magento:mftf:DataGenerator/etc/dataProfileSchema.xsd">

    <entity name="status" type="status">
        <data key="isEnabled">1</data>
        <data key="isDisabled">0</data>
    </entity>

    <!--General Settings tab-->
    <entity name="defGeneralSettings" type="generalSettings">
        <data key="enableModule">1</data>
        <data key="title">FAQ</data>
        <data key="urlPrefix">knowledge-base</data>
        <data key="addFaqToToolbar">1</data>
        <data key="addFaqToCategoriesMenu">0</data>
        <data key="addFaqToFooter">1</data>
        <data key="allowGuestAskQuestions">0</data>
    </entity>

    <entity name="addCustomFaqLink" extends="defGeneralSettings">
        <data key="title" unique="suffix">FAQ-</data>
        <data key="urlPrefix">custom-knowledge-base</data>
        <data key="addFaqToCategoriesMenu">1</data>
    </entity>

    <entity name="notAddFaqLink" extends="defGeneralSettings">
        <data key="title" unique="suffix">FAQ-</data>
        <data key="urlPrefix">custom-knowledge-base</data>
        <data key="addFaqToToolbar">0</data>
        <data key="addFaqToFooter">0</data>
    </entity>

    <entity name="allowGuestAskQuestion" extends="defGeneralSettings">
        <data key="allowGuestAskQuestions">1</data>
    </entity>

    <!--FAQ Home Page tab-->
    <entity name="defFaqHomePage" type="homePage">
        <data key="title">FAQ Home Page</data>
    </entity>

    <!--FAQ Search, Navigation and Sorting tab-->
    <entity name="defFaqSearchNavigationSorting" type="search">
        <data key="breadcrumbs">1</data>
        <data key="askQuestionButton">1</data>
        <data key="sortCategoriesBy">position</data>
        <data key="sortQuestionsBy">position</data>
        <data key="answerLength">250</data>
        <data key="searchBox">1</data>
        <data key="noResultsText">No results found</data>
        <data key="questionsInCategory">20</data>
        <data key="questionsInSearch">15</data>
        <data key="shortAnswer">0</data>
        <data key="limitTagsInMenu">10</data>
    </entity>

    <entity name="notShowBreadcrumbs" extends="defFaqSearchNavigationSorting">
        <data key="breadcrumbs">0</data>
    </entity>

    <entity name="notShowAskQuestionButton" extends="defFaqSearchNavigationSorting">
        <data key="askQuestionButton">0</data>
    </entity>

    <entity name="sortCategoriesByName" extends="defFaqSearchNavigationSorting">
        <data key="sortCategoriesBy">name</data>
    </entity>

    <entity name="sortQuestionsByName" extends="defFaqSearchNavigationSorting">
        <data key="sortQuestionsBy">name</data>
    </entity>

    <entity name="twoQuestionInCategory" extends="defFaqSearchNavigationSorting">
        <data key="questionsInCategory">2</data>
    </entity>

    <entity name="twoQuestionInSearchResult" extends="defFaqSearchNavigationSorting">
        <data key="questionsInSearch">2</data>
    </entity>

    <entity name="limitDisplayedAnswerLength" extends="defFaqSearchNavigationSorting">
        <data key="answerLength">15</data>
    </entity>

    <entity name="noShowSearchBox" extends="defFaqSearchNavigationSorting">
        <data key="searchBox">0</data>
    </entity>

    <entity name="noResultsText" extends="defFaqSearchNavigationSorting">
        <data key="noResultsText">No results found. Custom text</data>
    </entity>

    <entity name="cutFullAnswer" extends="defFaqSearchNavigationSorting">
        <data key="answerLength">15</data>
        <data key="shortAnswer">1</data>
    </entity>

    <entity name="oneTagInMenu" extends="defFaqSearchNavigationSorting">
        <data key="limitTagsInMenu">1</data>
    </entity>

    <!--Product Page tab-->
    <entity name="defQuestionTab" type="productPage">
        <data key="showTab">1</data>
        <data key="nameConfig"></data>
        <data key="name">Product Questions</data>
        <data key="positionConfig">55</data>
        <data key="position">2</data>
        <data key="showAskQuestionButton">1</data>
        <data key="limitQuestions">15</data>
        <data key="shortAnswer">1</data>
    </entity>

    <entity name="disableQuestionTab" extends="defQuestionTab">
        <data key="showTab">0</data>
    </entity>

    <entity name="customTabNameAndPosition" extends="defQuestionTab">
        <data key="nameConfig">Custom Tab Name {count}</data>
        <data key="name">Custom Tab Name</data>
        <data key="positionConfig">1</data>
        <data key="position">1</data>
    </entity>

    <entity name="questionQty" type="qty">
        <data key="oneQuestion">(1)</data>
        <data key="twoQuestion">(2)</data>
        <data key="threeQuestion">(3)</data>
    </entity>

    <entity name="notShowAskQuestionButtonProdPage" extends="defQuestionTab">
        <data key="showAskQuestionButton">0</data>
    </entity>

    <entity name="twoQuestionsOnProductPage" extends="defQuestionTab">
        <data key="limitQuestions">2</data>
    </entity>

    <entity name="showShortAnswerOnProductPage" extends="defQuestionTab">
        <data key="shortAnswer">0</data>
    </entity>

    <!--Rating tab-->
    <entity name="defRating" type="rating">
        <data key="isEnabled">1</data>
        <data key="type">0</data>
        <data key="allowForGuest">0</data>
    </entity>

    <entity name="yesNoRating" type="rating">
        <data key="isEnabled">1</data>
        <data key="type">0</data>
        <data key="allowForGuest">1</data>
    </entity>

    <entity name="voting" type="rating">
        <data key="isEnabled">1</data>
        <data key="type">1</data>
        <data key="allowForGuest">1</data>
    </entity>

    <entity name="averageNotHideZero" type="rating">
        <data key="isEnabled">1</data>
        <data key="type">2</data>
        <data key="hideZeroTotal">0</data>
        <data key="allowForGuest">1</data>
    </entity>

    <entity name="averageHideZero" extends="averageNotHideZero">
        <data key="hideZeroTotal">1</data>
    </entity>

    <entity name="disableRating" type="rating">
        <data key="isEnabled">0</data>
    </entity>

    <!--Social Buttons tab-->
    <entity name="defSocialButtons" type="socialButtons">
        <data key="isEnabled">1</data>
        <data key="socialNetworks">facebook,twitter,reddit,tumblr</data>
    </entity>

    <entity name="allSocialButtons" type="socialButtons">
        <data key="isEnabled">1</data>
        <data key="socialNetworks">email,whatsapp,facebook,twitter,reddit,tumblr,google+,digg,stumbleupon,linkedin,vkontakte,buffer</data>
    </entity>

    <entity name="disableSocialButtons" type="socialButtons">
        <data key="isEnabled">0</data>
    </entity>

    <!--SEO tab-->
    <entity name="defSeo" type="seo">
        <data key="canonicalUrl">0</data>
        <data key="structuredData">0</data>
        <data key="richDataBreadcrumbs">0</data>
        <data key="richDataOrganization">0</data>
    </entity>

    <entity name="enableCanonicalUrl" extends="defSeo">
        <data key="canonicalUrl">1</data>
    </entity>

    <entity name="enableStructuredData" extends="defSeo">
        <data key="structuredData">1</data>
    </entity>

    <entity name="enableRichDataBreadcrumbs" extends="defSeo">
        <data key="richDataBreadcrumbs">1</data>
    </entity>

    <!--GDPR Consent tab-->
    <entity name="defGdpr" type="gdpr">
        <data key="isEnabled">0</data>
    </entity>

    <entity name="enableGdpr" type="gdpr">
        <data key="isEnabled">1</data>
    </entity>
</entities>
